doctype html
html(lang="en").no-js
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title Luis Rosario
    meta(name="description", content="testing terminal.css with source sans pro")
    link(rel="preload", href="css/styles.css", as="style")
    link(rel="stylesheet", href="css/styles.css")

  body

    a(href="#main").show-for-sr Skip to main content

    .container
      header
        h1.f4 Luis Rosario
        p.lead Web Developer
        p <abbr title="Hyper-Text Markup Language">HTML</abbr>, <abbr title="Cascading Style Sheets">CSS</abbr>, <abbr title="JavaScript">JS</abbr>

      main#main
        p #[svg.feather #[use(xlink:href="assets/fonts/feather-sprite.svg#map-pin")]] Rochester, NY
        p.measure Born in Mexico, raised in the states. I help make sure websites at the University of Rochester are accessible, performant, and beautiful.
        ul
          li
            a(target="_blank", href="https://github.com/Z4P0/") Github
          li
            a(target="_blank", href="https://twitter.com/zapo__") Last.fm
          li
            a(target="_blank", href="https://www.last.fm/user/space_monk-e") Twitter
